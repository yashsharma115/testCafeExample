{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run TestCafe Test",
            "program": "${workspaceFolder}/node_modules/testcafe/bin/testcafe.js",
            "args": [
                "chrome",
                "${workspaceFolder}/tests/sshCommand.js"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}
